<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufpb.dcx.apps4society.educapimanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ui.challenge.ChallengesByContextActivity"></activity>
        <activity
            android:name=".view.ui.challenge.CreateChallengeActivity"
            android:parentActivityName=".view.NavDrawerActivity"></activity>
        <activity
            android:name=".view.ui.context.CreateContextActivity"
            android:parentActivityName=".view.NavDrawerActivity"></activity>
        <activity android:name=".view.RegisterActivity"></activity>
        <activity android:name=".view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.NavDrawerActivity"
            android:label="@string/title_activity_nav_drawer" />
    </application>

</manifest>