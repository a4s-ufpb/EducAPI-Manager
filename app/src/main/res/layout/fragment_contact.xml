<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_shadow_withe"
            android:layout_margin="10dp"
            >
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/ayla"
                android:layout_margin="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:text="@string/coordenadora"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="20dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="50dp"
                android:text="@string/coordenadoraEmail"
                android:textColor="@color/slogan_app_color"
                android:textSize="14sp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/coordenadoraGit"
                android:autoLink="all"
                android:clickable="true"
                android:focusable="true"
                android:textColor="@android:color/holo_blue_dark"
                android:textColorLink="@android:color/holo_blue_dark"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="150dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/github"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="80dp"/>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_shadow_withe"
            android:layout_margin="10dp"
            >
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/emerson"
                android:layout_margin="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:text="@string/dev1"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="20dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="50dp"
                android:text="@string/devEmail1"
                android:textColor="@color/slogan_app_color"
                android:textSize="14sp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dev1Git"
                android:autoLink="all"
                android:clickable="true"
                android:focusable="true"
                android:textColor="@android:color/holo_blue_dark"
                android:textColorLink="@android:color/holo_blue_dark"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="150dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/github"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="120dp" />
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_shadow_withe"
            android:layout_margin="10dp"
            >
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/francivaldo"
                android:layout_margin="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:text="@string/dev3"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="50dp"
                android:text="@string/dev3Email"
                android:textColor="@color/slogan_app_color"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dev3Git"
                android:singleLine="true"
                android:autoLink="all"
                android:clickable="true"
                android:focusable="true"
                android:textColor="@android:color/holo_blue_dark"
                android:textColorLink="@android:color/holo_blue_dark"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="150dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/github"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="120dp" />
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_shadow_withe"
            android:layout_margin="10dp"
            >
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/vitor"
                android:layout_margin="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:text="@string/dev"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="20dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="50dp"
                android:text="@string/devEmail"
                android:textColor="@color/slogan_app_color"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/devGit"
                android:autoLink="all"
                android:clickable="true"
                android:focusable="true"
                android:textColor="@android:color/holo_blue_dark"
                android:textColorLink="@android:color/holo_blue_dark"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="150dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/github"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="80dp"/>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_shadow_withe"
            android:layout_margin="10dp"
            >
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/raimundo"
                android:layout_margin="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:text="@string/dev2"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="20dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="120dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="50dp"
                android:text="@string/dev2Email"
                android:textColor="@color/slogan_app_color"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dev2Git"
                android:autoLink="all"
                android:clickable="true"
                android:focusable="true"
                android:textColor="@android:color/holo_blue_dark"
                android:textColorLink="@android:color/holo_blue_dark"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="150dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/github"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="80dp"
                android:layout_marginStart="120dp" />
        </com.google.android.material.card.MaterialCardView>





    </LinearLayout>
</androidx.core.widget.NestedScrollView>